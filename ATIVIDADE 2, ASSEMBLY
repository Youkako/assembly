.data 
  msg1:. asciiz"forneça o primeiro numero decimal\n"
  msg2:. asciiz"forneça o segundo numero decimal\n"
  msg3:. asciiz"a soma dos numeros é"
.text
  li $v0, 4 
  la $a0, msg1
  syscall

  li $v0, 6
  syscall

  mov.s $f3, $f0

  li $v0, 4
  la $a0, msg2
  syscall

  li $v0, 6
  syscall

  mov.s $f4, $f0
  add.s $f12, $f3, $f4

  li $v0, 4 
  la $a0, msg3
  syscall

  li $v0, 2
  syscall

  li $v0,10
  syscall
